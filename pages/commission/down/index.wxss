.row-remark {
  text-align: right;
  position: relative;
  -webkit-flex-shrink: 0;
  flex-shrink: 0;
  padding-left: 20rpx;
  color: #aaa;
  font-size: 26rpx;
}

.row-remark:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  width: 1px;
  height: 100%;
  background: #eee;
}

.center {
  text-align: center;
}

.edtion {
  color: #999;
  font-size: 14px;
  text-align: center;
  padding: 20rpx 0;
}

.empty-tip {
  padding: 150rpx;
}

.light {
  height: 260rpx;
  width: 240rpx;
}

.text-cancel {
  padding: 40rpx;
}

.com-down .subtitle {
  font-size: 28rpx;
  color: #999;
}

.com-down .image-32 {
  width: 32rpx;
  height: 32rpx;
}

image {
  vertical-align: top;
}
